@article{Jones2003,
	Abstract = {While nuclear{\textendash}magnetic{\textendash}resonance (NMR) techniques are unlikely to lead to a large{\textendash}scale quantum computer, they are well suited to investigating basic phenomena and developing new techniques. Indeed, it is likely that many existing NMR techniques will find uses in quantum information processing. Here I describe how the composite{\textendash}rotation (composite{\textendash}pulse) method can be used to develop quantum logic gates which are robust against systematic errors.},
