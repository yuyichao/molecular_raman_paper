	Pages = {510--513},
	Publisher = {American Association for the Advancement of Science},
	Title = {Spectroscopic observation of resonances in the F + H2 reaction},
	Url = {http://science.sciencemag.org/content/349/6247/510},
	Volume = {349},
	Year = {2015},
	Bdsk-Url-1 = {http://science.sciencemag.org/content/349/6247/510},
