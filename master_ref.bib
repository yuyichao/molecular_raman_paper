keywords = {Stark effect; Laser trapping; Optical tweezers or optical manipulation; Circular polarization; Electric fields; Hyperfine structure; Light beams; Light intensity; Magnetic fields},
number = {16},
pages = {19821--19825},
publisher = {OSA},
title = {Elliptical polarization for molecular Stark shift compensation in deep optical traps},
volume = {26},
month = {Aug},
year = {2018},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-26-16-19821},
doi = {10.1364/OE.26.019821},
abstract = {In optical dipole traps, the excited rotational states of a molecule may experience a very different light shift than the ground state. For particles with two polarizability components (parallel and perpendicular), such as linear 1\&\#x003A3; molecules, the differential shift can be nulled by choice of elliptical polarization. When one component of the polarization vector is \&\#x000B1;i2 times the orthogonal component, the light shift for a sublevel of excited rotational states \&\#x000B1;approaches that of the ground state at high optical intensity. In this case, fluctuating trap intensity need not limit coherence between ground and excited rotational states.},
}



