@article{Kinast2004a,
	Abstract = {We observe collective oscillations of a trapped, degenerate Fermi
    gas of6Li atoms at a magnetic field just above a {Feshbach} resonance,
    where thetwo-body physics does not support a bound state. The gas
    exhibits a radialbreathing mode at a frequency of 2837(05) Hz, in
    excellent agreement withthe frequency of \nu_H = 2830(20) Hz predicted
    for a hydrodynamic Fermigas with unitarity-limited interactions.
    The measured damping times andfrequencies are inconsistent with predictions
    for both the collisionlessmean field regime and for collisional hydrodynamics.
    These observationsprovide the first evidence for superfluid hydrodynamics
    in a resonantlyinteracting Fermi gas.},
