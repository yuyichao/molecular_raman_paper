	Pages = {1370--1375},
	Publisher = {American Association for the Advancement of Science},
	Title = {Synthesis of mixed hypermetallic oxide BaOCa+ from laser-cooled reagents in an atom-ion hybrid trap},
	Url = {http://science.sciencemag.org/content/357/6358/1370},
	Volume = {357},
	Year = {2017},
	Bdsk-Url-1 = {http://science.sciencemag.org/content/357/6358/1370},
